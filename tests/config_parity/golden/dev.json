{
  "app": {
    "name": "cli-multi-rapid",
    "log_level": "DEBUG",
    "debug": true,
    "api_port": 8000
  },
  "workflow": {
    "enabled": true,
    "max_concurrent": 2,
    "timeout_seconds": 3600
